{
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "**/index.js",
    "**/interfaces/*.js"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "check-coverage": true,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "all": true
}